<% include ./partials/header.ejs %>
<style>
  #posts::-webkit-scrollbar {
    display: none;
  }
</style>


<div class="w-full overflow-y-scroll h-screen bg-zinc-300 p-1" id="posts">
  <ul>
    <% users.forEach(function(elem){%>
    <li class="bg-zinc-200 mt-3 p-3 rounded-md flex justify-between items-center flex-wrap">
      <div class="flex gap-1 items-center">
      <img src="/images/uploads/<%= elem.profileImage%>" alt="" class="w-[50px] h-[50px] rounded-full inline-block mr-[5px]">
      <span class="text-black font-bold block"><%= elem.username%></span>
    </div>
      <div class="w-12 sm:14 rounded-md overflow-hidden hover:opacity-50 active:opacity-40 cursor-pointer" onclick="showAlert()"><img src="data:image/jpeg;base64," alt="message icon" class="w-full h-full"></div>
    </li>
    <%})%>
  </ul>
</div>

<script>
  function showAlert() {
    Swal.fire({
      title: '<strong>Sorry,</strong>',
      icon: 'info',
      html: '<b>This website is developing and the message service is not available!!</b>',
      
      showCancelButton: true,
      focusConfirm: false,
      
    });
  }


  
</script>
<% include ./partials/footer.ejs %>